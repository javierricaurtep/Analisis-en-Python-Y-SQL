        "pregunta7 = '''\n",
        "SELECT Id,\n",
        "       CASE\n",
        "           WHEN instr(CORREO, '@') > 0 THEN 'Consistente'\n",
        "           ELSE 'Nulo'\n",
        "       END AS Validacion_Correo\n",
        "FROM Estudiantes;\n",
        "'''\n",
        "resultado7 = pd.read_sql(pregunta7, engine)\n",
