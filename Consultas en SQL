        "SELECT substr(FECHAINGRESO, -4) AS Año_Ingreso, CARRERA, COUNT(Id) AS Cantidad_Estudiantes\n",
        "FROM Estudiantes\n",
        "GROUP BY Año_Ingreso, CARRERA\n",
        "HAVING Año_Ingreso IS NOT NULL;\n",
        "'''\n",
